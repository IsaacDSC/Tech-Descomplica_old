{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "sourceMap": true,
      "allowJs": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "paths": {
        "@modules/*": [
          "Modules/*"
        ],
        "@models/*": [
          "Models/*"
        ]
      },
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    }
  }